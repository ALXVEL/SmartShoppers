      <Button layoutX="150.0" layoutY="342.0" mnemonicParsing="false" onAction="#removeFromShoppingList" text="Remove from Shopping List" />
      <Button layoutX="248.0" layoutY="74.0" mnemonicParsing="false" onAction="#refreshButton" text="Refresh" />
   </children>
</AnchorPane>
