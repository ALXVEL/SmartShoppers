      <TableView fx:id="customerTable" layoutX="16.0" layoutY="104.0" prefHeight="223.0" prefWidth="295.0" />
      <CheckBox fx:id="onSale" layoutX="16.0" layoutY="78.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="67.0" text="on Sale" />
